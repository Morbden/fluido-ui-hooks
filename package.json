{
  "name": "@fluido/ui-hooks",
  "version": "0.5.1-beta.12",
  "license": "MIT",
  "private": false,
  "description": "A framework for create front-end easy and fast",
  "keywords": [
    "fluido",
    "front-end",
    "styled",
    "styled-components",
    "goober",
    "chakra",
    "react",
    "hooks",
    "framework",
    "layout"
  ],
  "author": {
    "email": "morbden.dev@gmail.com",
    "name": "Morbden"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Morbden/fluido-ui-hooks"
  },
  "main": "common/index.js",
  "module": "esm/index.js",
  "types": "types/index.d.js",
  "scripts": {
    "dev": "run-p dev:common dev:esm",
    "dev:common": "tsc -w",
    "dev:esm": "tsc --build tsconfig.esm.json -w",
    "build": "run-s clear build:common build:esm",
    "clear": "rimraf common esm types",
    "build:common": "tsc",
    "build:esm": "tsc --build tsconfig.esm.json",
    "pub": "npm publish --access public",
    "pub:beta": "npm publish --access public --tag beta",
    "pub:alpha": "npm publish --access public --tag alpha",
    "pub:all": "run-s build pub"
  },
  "dependencies": {
    "@popperjs/core": "^2.10.1",
    "deep-is": "^0.1.4",
    "deepmerge": "^4.2.2",
    "react-hotkeys-hook": "^3.4.3",
    "react-popper": "^2.2.5",
    "use-double-tap": "^1.3.3",
    "use-resize-observer": "^8.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0 <=17.x.x",
    "react-dom": ">=16.8.0 <=17.x.x"
  },
  "devDependencies": {
    "@types/node": "^16.10.1",
    "@types/react": "^17.0.24",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.3"
  }
}
